адне програмування», «Метрологія та інформаційно-вимірювальна техніка» для подальшої професійної діяльності у галузі автоматизації та приладобудування.","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"marks":[],"value":"При підготовці фахівців застосовуються ","nodeType":"text"},{"data":{},"marks":[{"type":"bold"}],"value":"STEM-орієнтовані навчальні робочі програми","nodeType":"text"},{"data":{},"marks":[],"value":", STEM означає ","nodeType":"text"},{"data":{},"marks":[{"type":"bold"}],"value":"S","nodeType":"text"},{"data":{},"marks":[],"value":"cience (наука), ","nodeType":"text"},{"data":{},"marks":[{"type":"bold"}],"value":"T","nodeType":"text"},{"data":{},"marks":[],"value":"echnology (технології), ","nodeType":"text"},{"data":{},"marks":[{"type":"bold"}],"value":"E","nodeType":"text"},{"data":{},"marks":[],"value":"ngineering (інженерія), ","nodeType":"text"},{"data":{},"marks":[{"type":"bold"}],"value":"M","nodeType":"text"},{"data":{},"marks":[],"value":"athematics (математика).","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"marks":[],"value":"Циклова комісія комп’ютерно-інтегрованих технологій та метрології – це простір інновацій для всестороннього розвитку особистості кожного.","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"marks":[],"value":"Найкращий спосіб передбачити майбутнє – це створити його самому!","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"marks":[{"type":"bold"}],"value":"Створюйте майбутнє з нами!","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"document"},"staff":[{"metadata":{"tags":[]},"sys":{"space":{"sys":{"type":"Link","linkType":"Space","id":"kwjkl9590afm"}},"id":"7DBTD3VOzKsxT2e88a754P","type":"Entry","createdAt":"2021-05-17T08:08:06.169Z","updatedAt":"2021-05-17T08:52:54.997Z","environment":{"sys":{"id":"master","type":"Link","linkType":"Environment"}},"revision":2,"contentType":{"sys":{"type":"Link